{
  "name": "gympro5",
  "version": 2,
  "builds": [
    {
      "src": "gympro5-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/gympro5-web/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://gym-pro5.vercel.app/api",
    "REACT_APP_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/gympro5-web/build/index.html"
    }
  ]
}
